#include <iostream>
#include <iomanip>
using namespace std;


int main()
{
int num, i, j, count;
cout<<"Enter a Value: ";
cin>>num;

for(i=2;i<=num;i++)
{
    count=0;
    for(j=2;j<i;j++)
    {
        if(i%j==0){
        count++;
        break;
        }
    }
    if(count==0)
    {
        cout<< i<<setw(6);
    }
}
}
